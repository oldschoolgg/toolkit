{
	"name": "@oldschoolgg/toolkit",
	"version": "0.0.24",
	"description": "Oldschoolgg Toolkit",
	"main": "dist/index.js",
	"scripts": {
		"lint": "biome check --write --unsafe --diagnostic-level=error",
		"test:lint": "biome check --diagnostic-level=error",
		"build:tsc": "tsc -p src",
		"build": "yarn wipedist && yarn build:tsc",
		"wipedist": "rimraf \"dist/\"",
		"test": "concurrently \"tsc -p src\" \"yarn test:lint\" \"yarn test:unit\"",
		"test:unit": "vitest run --coverage",
		"dev": "yarn wipedist && tsc -w -p src",
		"test:watch": "vitest --coverage"
	},
	"dependencies": {
		"deep-merge": "^1.0.0",
		"e": "^0.2.33",
		"emoji-regex": "^10.2.1",
		"ioredis": "^5.4.1",
		"math-expression-evaluator": "^1.3.14",
		"zod": "^3.23.8"
	},
	"peerDependencies": {
		"discord.js": "^14.15.3",
		"oldschooljs": "^2.5.4"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.8.3",
		"@oldschoolgg/ts-config": "^0.0.1",
		"@types/math-expression-evaluator": "^1.2.2",
		"@types/node": "^20.14.9",
		"@vitest/coverage-v8": "^1.6.0",
		"concurrently": "^8.2.2",
		"discord.js": "^14.15.3",
		"oldschooljs": "^2.5.4",
		"rimraf": "^5.0.7",
		"typescript": "^5.5.3",
		"vitest": "^1.6.0"
	},
	"engines": {
		"node": ">=18.12.0"
	}
}
